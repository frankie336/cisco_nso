
<config-template xmlns="http://tail-f.com/ns/config/1.0">
  <devices xmlns="http://tail-f.com/ns/ncs">
    <device>
       <device>
        <name>{/device}</name>
        <config>
          <ip xmlns="urn:ios">
            <vrf>
              <name>{/vrf-name}</name>
              <description>{/vrf-description}</description>
              <rd>{/rd}</rd>
              <route-target>
                <export>
                  <asn-ip>{/export}</asn-ip>
                </export>
                <import>
                  <asn-ip>{/import}</asn-ip>
                </import>
              </route-target>
            </vrf>
          </ip>
        </config>
      </device> 
  </devices>
</config-template>





    